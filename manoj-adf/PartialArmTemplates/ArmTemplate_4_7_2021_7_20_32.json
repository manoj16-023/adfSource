{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "manoj-adf"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/SqlServerOnPremiseTableLS')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"parameters": {
					"ServerName": {
						"type": "String",
						"defaultValue": "DESKTOP-555G6B5\\MANOJ"
					},
					"DataBaseName": {
						"type": "String",
						"defaultValue": "TrainingDB"
					},
					"WindowsUsername": {
						"type": "String",
						"defaultValue": "DESKTOP-555G6B5\\Manoj kumar"
					},
					"WPassword": {
						"type": "String",
						"defaultValue": "qwert67890"
					}
				},
				"annotations": [],
				"type": "SqlServer",
				"typeProperties": {
					"connectionString": {
						"type": "SecureString",
						"value": "**********"
					},
					"userName": "@{linkedService().WindowsUsername}",
					"password": "@{linkedService().WPassword}"
				},
				"connectVia": {
					"referenceName": "OnPremise-SelfHosted",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": []
		}
	]
}